
@{
    ViewBag.Title = "Student";
}

<h2>Student</h2>
<div>
    <form action="" method="post">
        <div class="form-group">
            <label for="Ma">Ma Sinh Vien</label>
            <input type="text" class="form-control" placeholder="Enter Ma Sinh Vien" id="Ma">
        </div>
        <div class="form-group">
            <label for="fines">Fines</label>
            <input type="text" class="form-control" placeholder="Enter Fines" id="fines">
        </div>
        <div class="form-group">
            <label for="money">Money</label>
            <input type="text" class="form-control" placeholder="Enter Money" id="money">
        </div>
        <div class="form-group">
            <label for="penalty">Penalty</label>
            <input type="text" class="form-control" placeholder="Enter Penalty" id="penalty">
        </div>
        <button type="submit" class="btn btn-primary">Submit</button>
        <button type="reset" class="btn btn-default">Reset</button>
    </form>
</div>

